  "mtgmelee": {
    "api": {
      "base_url": "https://api.mtgmelee.com/v1",
      "endpoints": {
        "tournaments": "/tournaments",
        "tournament_details": "/tournaments/{tournament_id}",
        "tournament_standings": "/tournaments/{tournament_id}/standings",
        "tournament_pairings": "/tournaments/{tournament_id}/pairings",
        "tournament_decklists": "/tournaments/{tournament_id}/decklists",
        "decklist": "/decklists/{decklist_id}"
      },
      "auth": {
        "login_url": "https://api.mtgmelee.com/v1/auth/login",
        "token_refresh_url": "https://api.mtgmelee.com/v1/auth/refresh"
      }
    },
    "formats": {
      "standard": 1,
      "modern": 2,
      "pioneer": 3,
      "legacy": 4,
      "vintage": 5,
      "pauper": 6,
      "commander": 7,
      "brawl": 8,
      "historic": 9,
      "explorer": 10,
      "alchemy": 11
    },
    "api_config": {
      "rate_limit": {
        "requests_per_minute": 60,
        "requests_per_hour": 1000
      },
      "max_retries": 3,
      "retry_delay": 5,
      "timeout": 30,
      "max_days_back": 30
    }
  },
  "cache": {
    "raw_cache_path": "data-collection/raw-cache",
    "processed_cache_path": "data-collection/processed-cache",
    "file_format": "json",
    "max_cache_size_mb": 5000,
    "cleanup_threshold_mb": 4500,
    "cleanup_strategy": "oldest_first"
  },
  "logging": {
    "log_level": "INFO",
    "log_file": "logs/manalytics.log",
    "max_log_size_mb": 10,
    "backup_count": 5,
    "console_output": true
  }
}